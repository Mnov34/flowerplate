{
	"name": "HTTP Server",
	"imports": [
		"import http.server"
	],
	"code": [
		"handler = http.server.SimpleHTTPRequestHandler",
		"httpd = http.server.HTTPServer((\"\", 8000), handler)",
		"",
		"print(\"Server started at port 8000\")",
		"",
		"httpd.serve_forever()"
	],
	"language": "python",
	"tags": [
		"web",
		"server"
	],
	"variables": {
		"port": {
			"default": 8000,
			"type": "number"
		}
	}
}